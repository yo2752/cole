                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Apellidos" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="0"/>
                        <xs:maxLength value="120"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Documento_Identificacion">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="Numero" type="tipoDOI"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="Correo_Electronico" minOccurs="0">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:minLength value="0"/>
                        <xs:maxLength value="250"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Asunto">
        <xs:sequence>
            <xs:element name="Codigo">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="BTETC"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Descripcion">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration
                            value="Solicitud de Baja Telem√°tica por Exportaci√≥n o Tr√°nsito Comunitario"
                        />
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Destino">
        <xs:sequence>
            <xs:element name="Codigo">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="101001"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="Descripcion">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="DGT - Veh√≠culos"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <!--Tipos simples-->
    <xs:simpleType name="tipoDOItitular">
        <xs:restriction base="xs:string">
            <xs:pattern
                value="[kKlL0-9]{1}[0-9]{7}[a-zA-Z]{1}|[mMxXyYzZ]{1}[0-9]{7}[a-zA-Z]{1}|[a-hA-HjJuUvV]{1}[0-9]{8}|[p-sP-SnNwW]{1}[0-9]{7}[a-jA-J]{1}|[A-Z]\d{7}[A-Z]"
            />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tipoDOI">
        <xs:restriction base="xs:string">
            <xs:pattern
                value="[kKlL0-9]{1}[0-9]{7}[a-zA-Z]{1}|[mMxXyYzZ]{1}[0-9]{7}[a-zA-Z]{1}|[a-hA-HjJuUvV]{1}[0-9]{8}|[p-sP-SnNwW]{1}[0-9]{7}[a-jA-J]{1}"
            />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tipoDOI_Remitente">
        <xs:restriction base="xs:string">
            <xs:pattern
                value="[kKlL0-9]{1}[0-9]{7}[a-zA-Z]{1}|[a-hA-HjJuUvV]{1}[0-9]{8}|[p-sP-SnNwW]{1}[0-9]{7}[a-jA-J]{1}"
            />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tipoFechaAAAAMMDD">
        <xs:restriction base="xs:string">
            <xs:pattern value="(([1-2][0-9]{3})(0[1-9]|1[0-2])(0[1-9]|[1-2][0-9]|3[0-1]))"/>
        </xs:restriction>
    </xs:simpleType>
	<xs:simpleType name="tipoTasa">
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="0"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:length value="12"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>
    <xs:simpleType name="tipoTramite">
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tipoMotivo">
        <xs:restriction base="xs:string">
            <xs:length value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tipoFirma">
        <xs:restriction base="xs:base64Binary">
            <xs:minLength value="1"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tipoTextoLegal">
        <xs:restriction base="xs:string">
            <xs:enumeration
                value="Este Colegio de Gestores Administrativos ha verificado que la solicitud de baja definitiva por Exportaci√≥n o Tr√°nsito Comunitario presentada cumple con todas las obligaciones establecidas en el Reglamento General de Veh√≠culos y resto de la normativa aplicable, as√≠ como las derivadas de la aplicaci√≥n de las Instrucciones de la Direcci√≥n General de tr√°fico vigentes en el momento de la solicitud."
            />
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tipoSI">
        <xs:restriction base="xs:string">
            <xs:enumeration value="SI"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="tipoOrganismo">
        <xs:restriction base="xs:string">
            <xs:enumeration value="DGT"/>
        </xs:restriction>
    </xs:simpleType>
</xs:schema>
